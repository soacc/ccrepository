		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>





















	<managed-bean>
		<description>portletbean</description>
		<display-name>allinone</display-name>
		<managed-bean-name>Allinone</managed-bean-name>
		<managed-bean-class>com.webmethods.caf.allinone.Allinone</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<description>Specifies whether the session bean expires when the pageflow is out of scope</description>
			<display-name>Expire With Page Flow</display-name>
			<property-name>expireWithPageFlow</property-name>
			<property-class>java.lang.Boolean</property-class>
			<value>true</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<description>pagebean</description>
		<display-name>Allinone/default</display-name>
		<managed-bean-name>AllinoneDefaultviewView</managed-bean-name>
		<managed-bean-class>com.webmethods.caf.allinone.AllinoneDefaultviewView</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<description>Specifies whether the session bean expires when the pageflow is out of scope</description>
			<display-name>Expire With Page Flow</display-name>
			<property-name>expireWithPageFlow</property-name>
			<property-class>java.lang.Boolean</property-class>
			<value>true</value>
		</managed-property>
	</managed-bean>
	<navigation-rule>
		<from-view-id>/Allinone/*</from-view-id>
		<navigation-case>
			<from-outcome>default</from-outcome>
			<to-view-id>/Allinone/default.view</to-view-id>
		</navigation-case>
	</navigation-rule>
